<h1>ログイン</h1>
<%= form_with(scope: :session, url: sessions_path, local: true) do |f| %>
<div class="email">
  <%= f.label :email, "メールアドレス" %>
  <%= f.email_field :email %>
</div>
<div class="">
  <%= f.label :password, "パスワード" %>
  <%= f.password_field :password %>
</div>
<div class="text-left">
  <%= f.submit "ログイン", class: "btn btn-primary" %>
<% end %>
</div>
